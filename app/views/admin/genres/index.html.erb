<%= render "admin/shared/header" %>
<h1>ジャンル一覧とジャンル登録</h1>

<%= form_with model: @genre, url: '/admin/genres', method: :post do |f| %>

   <div class="form-group">
    <label for="item_name">ジャンル名</label>
      <%= f.text_field :name %>
   </div>

<% end %>

<h1>ここに一覧</h1>

<% @genres.each do |genre| %>

    <%= genre.name %>

<% end %>

